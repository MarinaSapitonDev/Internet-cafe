<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="TestTask.Model.CoffeeOrderItem" table="CoffeeOrderItem" catalog="test">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="type_id" type="java.lang.Integer">
            <column name="type_id" length="200" not-null="true"  />
        </property>
        <property name="order_id" type="java.lang.Integer">
            <column name="order_id"  not-null="true"  />
        </property>
        <property name="quantity" type="java.lang.Integer">
            <column name="quantity"  />
        </property>
    </class>
</hibernate-mapping>